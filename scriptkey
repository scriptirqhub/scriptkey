
















































-- Booting Library
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()


-- Functions
_G.Key = "🥵🥵"
_G.KeyInput = "string"

function DestroyGui()
    OrionLib:Destroy()
end

function Menu()
    loadstring(game:HttpGet('https://pastebin.com/raw/NaygGzGd'))()
end


-- Creating Window
local Window = OrionLib:MakeWindow({Name = "IRQ HUB💯🇮🇶", HidePremium = false, IntroEnabled = false})


-- Creating Tabs
local tab1 = Window:MakeTab({
	Name = "مـفـتـاح🔑",
	Icon = "",
	PremiumOnly = false
})

local tab2 = Window:MakeTab({
	Name = "كـيـف تـجـيـب مـفـتـاح✨💃",
	Icon = "",
	PremiumOnly = false
})


-- Config Tab1
local Section1 = tab1:AddSection({
	Name = "مـلاحـضه انـتـضـر قـليـلاً بـعـد الـتـشـغـيـل😐"
})

tab1:AddTextbox({
	Name = "ادخـل الـمـفـتاح هـنا🚀",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
         _G.KeyInput = Value
	end	  
})

tab1:AddButton({
	Name = "تـأكد مـن الـمـفـتـاح💯",
	Callback = function()loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptirqhub/IRQHUBV7/refs/heads/main/IRQHUBV7"))()
        if _G.KeyInput == _G.Key then
            OrionLib:MakeNotification({
	        Name = "تـــــــــــــم🔥",
	        Content = "تـم تـفـعـيل السكربت...... ",
	        Image = "",
	        Time = 1.5
            })
        wait(1.5)
        Menu()
    end
	end    
})

-- Config Tab2
local Section1 = tab2:AddSection({
	Name = "الـسـكـربـت لـلاعـضـاء فـقـط🔥🥵"
})






local Section1 = tab2:AddSection({
	Name = "اذا كـنـت تـريـد الـفـتـاح تـواصـل مـعـنى انسته✨✨"
})




local Section1 = tab2:AddSection({
	Name = "vene.221"
})
