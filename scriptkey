-- Booting Library
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()


-- Functions
_G.Key = "الملكي وبس"
_G.KeyInput = "string"

function DestroyGui()
    OrionLib:Destroy()
end

function Menu()
    loadstring(game:HttpGet('https://pastebin.com/raw/NaygGzGd'))()
end


-- Creating Window
local Window = OrionLib:MakeWindow({Name = "IRQ HUB🤯🔥", HidePremium = false, IntroEnabled = false})


-- Creating Tabs
local tab1 = Window:MakeTab({
	Name = "مـفـتـاح🔑",
	Icon = "",
	PremiumOnly = false
})

local tab2 = Window:MakeTab({
	Name = "كـيـف تـجـيـب مـفـتـاح✨💃",
	Icon = "",
	PremiumOnly = false
})

-- Config Tab1
local Section1 = tab1:AddSection({
	Name = "مـلاحـضه انـتـضـر قـليـلاً بـعـد الـتـشـغـيـل😐"
})

tab1:AddTextbox({
	Name = "ادخـل الـمـفـتاح هـنا🚀",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
         _G.KeyInput = Value
	end	  
})

tab1:AddButton({
	Name = "تـأكد مـن الـمـفـتـاح💯",
	Callback = function()loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptirqhub/brookhaven/refs/heads/main/IRQHUB"))()
        if _G.KeyInput == _G.Key then
            OrionLib:MakeNotification({
	        Name = "الـمـفـتـاح صـحـيـح!",
	        Content = "تـم تـفـعـيـل IRQ HUB🤯🔥",
	        Image = "",
	        Time = 2.5
            })
        wait(2.5)
        Menu()
    end
	end    
})

-- Config Tab2
local Section1 = tab2:AddSection({
	Name = "الـسـكـربـت لـلاعـضـاء فـقـط🔥🥵"
})


tab2:AddTextbox({
	Name = "رابـط سـيـرفـرنـه الـيـسـكـورد تـنـورنـه🔥//انسخ الرابط وحطه بقوقل",
	Default = "https://discord.gg/N8RD3nhh",
	TextDisappear = false,
	Callback = function(Value)
         _G.KeyInput = Value
	end	  
})



local Section1 = tab2:AddSection({
	Name = "اذا كـنـت تـريـد الـفـتـاح تـواصـل مـعـنى انسته✨✨"
})




local Section1 = tab2:AddSection({
	Name = "vene.221"
})
